<?xml version="1.0"?>
<package>
  <name>ohm_tsd_slam</name>
  <version>0.0.0</version>
  <description>The ohm_tsd_slam package provides a 2D SLAM approach for laser scanners. </description>


  <!-- One maintainer tag required, multiple allowed, one person per tag --> 
  <maintainer email="philipp.koch@th-nuernberg.de">Philipp Koch</maintainer>
  <maintainer email="markus.kuehn@th-nuernberg.de">Markus Kühn</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
  <license>TODO</license>


  <!-- Url tags are optional, but mutiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
  <url type="website">http://wiki.ros.org/ohm_tsd_slam</url>
  <url type="website">http://autonohm.de/</url>


  <!-- Author tags are optional, mutiple are allowed, one per tag -->
  <!-- Authors do not have to be maintianers, but could be -->
  <author >Philipp Koch</author>
  <author >Stefan May</author>
  <author >Markus Kühn</author>


